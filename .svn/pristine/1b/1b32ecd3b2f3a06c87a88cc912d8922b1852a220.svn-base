app.service("APIService", function($http) {
    this.getDestinations = function () {
        var url = "/api/HotelBooking/GetDestinationsRSList";
        return $http.get(url).then(function(response) {
            return response.data;
        });
    };
    this.hotelavailability = function(sub) {
        return $http({
            method: "post",
            data: sub,
            contentType: "application/json; charset=utf-8",
            url: "/api/HotelBooking"
            
        });
    };
    this.hotelavailabilityFilter = function (sub) {
        return $http({
            method: "post",
            data: sub,
            contentType: "application/json; charset=utf-8",
            url: "/api/HotelBooking/PostSearch"

        });
    };
    this.showhotelselected = function (subselect) {
        return $http({
            method: "post",
            data: subselect,
            contentType: "application/json; charset=utf-8",
            url: "Home/HotelView"
        });
    };
