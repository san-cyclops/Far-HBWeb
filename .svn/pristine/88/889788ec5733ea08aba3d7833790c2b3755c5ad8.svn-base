.divselect {
    position: relative;
    display: inline-block;
    zoom: 1;
    *float: left;
    *display: inline;
    z-index: 0;
    background: #FFF;
    -webkit-border-radius: 3px 3px 3px 3px;
    -moz-border-radius: 3px 3px 3px 3px;
    -ms-border-radius: 3px 3px 3px 3px;
    -o-border-radius: 3px 3px 3px 3px;
    border-radius: 3px 3px 3px 3px
}
.divselect select::-ms-expand {
    display: none
}
.divselect select {
    min-width: 42px;
    background: #FFF;
    font-family: 'Lato', Arial, sans-serif;
    -webkit-border-radius: 3px 3px 3px 3px;
    -moz-border-radius: 3px 3px 3px 3px;
    -ms-border-radius: 3px 3px 3px 3px;
    -o-border-radius: 3px 3px 3px 3px;
    border-radius: 3px 3px 3px 3px;
    border: 1px solid #CCC;
    color: #000;
    width: auto;
    padding-left: 3px!important;
    padding-right: 20px!important;
    z-index: 0;
    background: transparent!important;
    padding-right: 23px!important\9;
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    margin: 0!important
}
.divselect:after {
    content: '\e601';
    color: #000;
    position: relative;
    top: 2px;
    right: 2px;
    margin-left: -18px;
    font-family: evoicons;
    z-index: -1;
    pointer-events: none
}
@-moz-document url-prefix() {
    .divselect .divselect {
        display: inline-table
    }
    .divselect .divselect select {
        text-indent: .01px;
        min-width: 45px;
        padding-right: 23px!important
    }
}
