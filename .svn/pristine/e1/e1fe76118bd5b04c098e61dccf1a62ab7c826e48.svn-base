@{
    ViewBag.Title = "RoomUnavailable";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link rel='stylesheet prefetch' href='http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css'>
<link rel='stylesheet prefetch' href='https://code.jquery.com/ui/1.9.1/themes/base/jquery-ui.css'>
<style> 
 
 .ui-datepicker td span, .ui-datepicker td a {
  text-align: center;
}

.ui-widget-content .ui-state-default {
  background: #b3b3b3 url(images/ui-bg_glass_75_e6e6e6_1x400.png) 50% 50% repeat-x;
}

.ui-state-default.ui-state-active {
  background: white url(images/ui-bg_glass_75_e6e6e6_1x400.png) 50% 50% repeat-x;
}

</style>

<body data-ng-app="RoomUnavailable" ng-controller="RoomCtrl">

    <div class="panel panel-yellow" id="page-wrapper">
        <div class=" panel-heading">
        Room Unavailable Form
    </div>

        <br /><br />

        <form action="#" class="form-horizontal">
            <div class="form-body pal">

                <div class="form-group">
                    <label for="inputroom" class="col-md-2 control-label"> Room ID </label>
                    <div class="col-md-2">
                        <div class="input-icon right">

                            <select class="form-control" data-ng-model="roomid"></select>

                        </div>

                    </div>

                    <label for="inputroom" class="col-md-2 control-label"> Month </label>
                    <div class="col-md-2">
                        <div class="input-icon right">

                            <select class="form-control">
                                <option> January</option>
                                <option> February</option>
                                <option> March</option>
                                <option> April</option>
                                <option> May</option>
                                <option> June</option>
                                <option> July</option>
                                <option> August</option>
                                <option> September</option>
                                <option> October</option>
                                <option> November</option>
                                <option> December</option>

                            </select>

                        </div>

                    </div>


                    <label for="inputDate" class="col-md-1 control-label"> Date </label>

                    <div class="col-md-2">
                        <div id="datepicker">
                            <input type="text" class="form-control" />
                        </div>
                    </div>

                    <div class="col-md-1">
                        <button type="submit" class="btn btn-primary" ng-click="add()">
                            Add
                        </button>

                    </div>
                </div>

                <br /><br />

                <div class="form-group">
                    <div class="col-md-3">

                    </div>
                    <div class="col-md-3">

                    </div>

                    <div class="col-md-3">
                        <button type="submit" class="btn btn-primary" ng-click="save()">
                            Save
                        </button>
                    </div>

                    <div class="col-md-3">

                    </div>

                </div>

            </div>
        </form>
    </div>
 
    <script data-require="angular.js@1.4.0" data-semver="1.4.0" src="https://code.angularjs.org/1.4.0/angular.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script> 
  
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src='http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js'></script>

    <script src="~/Scripts/RoomUnavailable/datepicker.js"></script>

    <script src="~/Scripts/RoomUnavailable/unavailable.js"></script>
    

   


</body>


</html>
